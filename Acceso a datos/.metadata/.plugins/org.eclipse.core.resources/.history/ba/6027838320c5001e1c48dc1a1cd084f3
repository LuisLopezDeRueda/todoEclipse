package Modelo;

public class Direccion {
	public Integer numero;
	public String calle;
	public Integer cp;

	public Direccion(Integer numero, String calle, Integer cp) {
		this.numero = numero;
		this.calle = calle;
		this.cp = cp;

	}
	public Direccion() {}

	public Integer getNumero() {
		return numero;
	}

	public void setNumero(Integer numero) {
		this.numero = numero;
	}

	public String getCalle() {
		return calle;
	}

	public void setCalle(String calle) {
		this.calle = calle;
	}

}
